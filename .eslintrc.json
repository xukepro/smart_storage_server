{
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true,
      "node": true
    },
    "extends": "eslint:recommended",
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
      "ecmaVersion": 2018
    },
    "rules": {
      "id-match": 0,
      "no-unused-vars": [
        0,
        {
          "vars": "all",
          "args": "none",
          "caughtErrors": "none"
        }
      ],
      "comma-spacing": [
        1,
        {
          "before": false,
          "after": true
        }
      ],
      "semi": [
        1,
        "always",
        {
          "omitLastInOneLineBlock": true
        }
      ],
      "no-extra-semi": 1,
      "no-mixed-spaces-and-tabs": 1,
      "indent": [
        1,
        2,
        {
          "SwitchCase": 1,
          "flatTernaryExpressions": true
        }
      ]
    }
  }